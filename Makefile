	test -d $(OUTDIR) || mkdir -p $(OUTDIR)
	test -d $(OBJDIR_MAKEDICT) || mkdir -p $(OBJDIR_MAKEDICT)
	test -d $(OBJDIR_MAKEDICT)/$(SRCDIR) || mkdir -p $(OBJDIR_MAKEDICT)/$(SRCDIR)

